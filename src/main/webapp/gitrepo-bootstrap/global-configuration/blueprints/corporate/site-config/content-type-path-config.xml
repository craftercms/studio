<!--  mapping for which content types are allowed at which paths -->
<!--  this file is no longer used and should be removed.  These paths are managed in the config.xml of each type -->
<content-type-path-mapping>
	<path>
		<!--  no path-include means match everything -->
		<path-includes/>
		<path-excludes/>
		<content-types>
			<content-type name="/global/page/article" is-wcm-type="true"/>
		</content-types>
	</path>
</content-type-path-mapping>

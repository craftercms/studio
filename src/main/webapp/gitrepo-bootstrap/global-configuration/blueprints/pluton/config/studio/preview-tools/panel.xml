        <module>
            <moduleName>medium-panel</moduleName>
            <title>publishingChannel</title>
            <config>
                <channels>
                    <channel>
                        <title>web</title>
                        <value>browser</value>
                        <width></width>
                        <height></height>
                    </channel>
                    <channel>
                        <title>smartPhone</title>
                        <value>iphone</value>
                        <width>320</width>
                        <height>568</height>
                    </channel>
                    <channel>
                        <title>tablet</title>
                        <value>ipad</value>
                        <width>768</width>
                        <height>1024</height>
                    </channel>
                </channels>
            </config>
        </module>
        <module>
            <moduleName>experiments-panel</moduleName>
            <title>multiVariantTesting</title>
            <config>
                <experiments>
                    <experiment>
                        <title>YellowVsBlue</title>
                        <variations>
                            <variation>
                                <title>yellow</title>
                                <url>/</url>
                            </variation>
                            <variation>
                                <title>blue</title>
                                <url>/?c1v1</url>
                            </variation>
                        </variations>
                    </experiment>


                    <experiment>
                        <title>FacetedVsKeyword</title>
                        <variations>
                            <variation>
                                <title>faceted</title>
                                <url>/</url>
                            </variation>
                            <variation>
                                <title>facetedKeyword</title>
                                <url>/?c1v1</url>
                            </variation>
                            <variation>
                                <title>keywordOnly</title>
                                <url>/?c1v1</url>
                            </variation>
                        </variations>
                    </experiment>

                </experiments>
            </config>
        </module>

<?xml version="1.0" encoding="UTF-8"?>
<dependency-resolver>
    <types>
        <dependency-type>
            <name>page</name>
            <includes>
                <path-pattern>/site/website/.*\.xml</path-pattern>
            </includes>
            <mimetypes>
                <mimetype>
                    <name>application/xml</name>
                    <patterns>
                        <pattern>/site/website/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>application/javascript</name>
                    <patterns>
                        <pattern>/site/website/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/css</name>
                    <patterns>
                        <pattern>/site/website/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/x-freemarker</name>
                    <patterns>
                        <pattern>/site/website/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
            </mimetypes>
        </dependency-type>
        <dependency-type>
            <name>component</name>
            <includes>
                <path-pattern>/site/components/.*\.xml</path-pattern>
                <path-pattern>/site/system/page-components/.*\.xml</path-pattern>
                <path-pattern>/site/component-bindings/.*\.xml</path-pattern>
                <path-pattern>/site/indexes/.*\.xml</path-pattern>
                <path-pattern>/site/resources/.*\.xml</path-pattern>
                <path-pattern>/site/items/.*\.xml</path-pattern>
            </includes>
            <mimetypes>
                <mimetype>
                    <name>application/xml</name>
                    <patterns>
                        <pattern>/site/components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/system/page-components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/component-bindings/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/indexes/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/resources/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>application/javascript</name>
                    <patterns>
                        <pattern>/site/components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/system/page-components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/component-bindings/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/indexes/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/resources/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/css</name>
                    <patterns>
                        <pattern>/site/components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/system/page-components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/component-bindings/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/indexes/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/resources/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/x-freemarker</name>
                    <patterns>
                        <pattern>/site/components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/system/page-components/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/component-bindings/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/indexes/([^&lt;]+)\.xml</pattern>
                        <pattern>/site/resources/([^&lt;]+)\.xml</pattern>
                    </patterns>
                </mimetype>
            </mimetypes>
        </dependency-type>
        <dependency-type>
            <name>asset</name>
            <includes>
                <path-pattern>/static-assets/.*</path-pattern>
            </includes>
            <mimetypes>
                <mimetype>
                    <name>application/xml</name>
                    <patterns>
                        <pattern>/static-assets/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>application/javascript</name>
                    <patterns>
                        <pattern>/static-assets/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/css</name>
                    <patterns>
                        <pattern>/static-assets/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/x-freemarker</name>
                    <patterns>
                        <pattern>/static-assets/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
            </mimetypes>
        </dependency-type>
        <dependency-type>
            <name>rendering-template</name>
            <includes>
                <path-pattern>/templates/.*\.ftl</path-pattern>
            </includes>
            <mimetypes>
                <mimetype>
                    <name>application/xml</name>
                    <patterns>
                        <pattern>/templates/([^&lt;"]+)\.ftl</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>application/javascript</name>
                    <patterns>
                        <pattern>/templates/([^&lt;"]+)\.ftl</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/css</name>
                    <patterns>
                        <pattern>/templates/([^&lt;"]+)\.ftl</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/x-freemarker</name>
                    <patterns>
                        <pattern>/templates/([^&lt;"]+)\.ftl</pattern>
                    </patterns>
                </mimetype>
            </mimetypes>
        </dependency-type>
        <dependency-type>
            <name>script</name>
            <includes>
                <path-pattern>/scripts/.*\.groovy</path-pattern>
            </includes>
            <mimetypes>
                <mimetype>
                    <name>application/xml</name>
                    <patterns>
                        <pattern>/scripts/([^&lt;"]+)\.groovy</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>application/javascript</name>
                    <patterns>
                        <pattern>/scripts/([^&lt;"]+)\.groovy</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/css</name>
                    <patterns>
                        <pattern>/scripts/([^&lt;"]+)\.groovy</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/x-freemarker</name>
                    <patterns>
                        <pattern>/scripts/([^&lt;"]+)\.groovy</pattern>
                    </patterns>
                </mimetype>
            </mimetypes>
        </dependency-type>
        <dependency-type>
            <name>document</name>
            <includes>
                <pattern>/site/documents/.*</pattern>
            </includes>
            <mimetypes>
                <mimetype>
                    <name>application/xml</name>
                    <patterns>
                        <pattern>/site/documents/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>application/javascript</name>
                    <patterns>
                        <pattern>/site/documents/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/css</name>
                    <patterns>
                        <pattern>/site/documents/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
                <mimetype>
                    <name>text/x-freemarker</name>
                    <patterns>
                        <pattern>/site/documents/([^&lt;"'\)]+)</pattern>
                    </patterns>
                </mimetype>
            </mimetypes>
        </dependency-type>
    </types>
</dependency-resolver>
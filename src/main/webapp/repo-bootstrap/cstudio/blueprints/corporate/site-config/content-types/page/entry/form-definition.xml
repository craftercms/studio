<form>
	<title>A Page</title>
	<description>A Simple Page</description>
	<objectType>page</objectType>
	<content-type>/page/entry</content-type>
	<properties>		<property>
			<name>placeInNav</name>
			<label>Show In Nav</label>
			<value>true</value>
			<type>boolean</type>
		</property>
		<property>
			<name>display-template</name>
			<label>Display Template</label>
			<value>/templates/web/entry.ftl</value>
			<type>template</type>
		</property>
		<property>
			<name>descriptor-mapper</name>
			<label>Descriptor Mapper</label>
			<value>hierarchical-mapper</value>
			<type>string</type>
		</property>
	</properties>
	<sections>		<section>
			<title>Page Settings</title>
			<description></description>
			<defaultOpen>true</defaultOpen>
			<fields>
				<field>
					<type>file-name</type>
					<id>file-name</id>
					<iceId></iceId>
					<title>page URL</title>
					<description>DESC</description>
					<defaultValue></defaultValue>
					<help>HELP</help>
					<properties>
						<property>
							<name>size</name>
							<value>50</value>
							<type>int</type>
						</property>
						<property>
							<name>maxlength</name>
							<value>50</value>
							<type>int</type>
						</property>
						<property>
							<name>path</name>
							<value></value>
							<type>string</type>
						</property>
						<property>
							<name>readonly</name>
							<value></value>
							<type>boolean</type>
						</property>
					</properties>
					<constraints>
					</constraints>
				</field>
				<field>
					<type>input</type>
					<id>internal-name</id>
					<iceId>optional</iceId>
					<title>Internal Title</title>
					<description>optional</description>
					<defaultValue></defaultValue>
					<help>optional</help>
					<properties>
						<property>
							<name>size</name>
							<value>50</value>
							<type>int</type>
						</property>
						<property>
							<name>maxlength</name>
							<value>60</value>
							<type>int</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[true]]></value>
							<type>int</type>
						</constraint>
					</constraints>
				</field>
			</fields>
		</section>
		<section>
			<title>Main Content</title>
			<description></description>
			<defaultOpen>true</defaultOpen>
			<fields>
				<field>
					<type>image-picker</type>
					<id>heroImage</id>
					<iceId>heroImage</iceId>
					<title>Hero Image</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
					<properties>
						<property>
							<name>width</name>
							<value>980</value>
							<type>int</type>
						</property>
						<property>
							<name>height</name>
							<value>540</value>
							<type>int</type>
						</property>
						<property>
							<name>imageManager</name>
							<value>homepage-heros</value>
							<type>datasource:image</type>
						</property>
						<property>
							<name>readonly</name>
							<value></value>
							<type>boolean</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[true]]></value>
							<type>boolean</type>
						</constraint>
					</constraints>
				</field>
				<field>
					<type>checkbox</type>
					<id>enableTouts</id>
					<iceId>touts</iceId>
					<title>Enable Touts</title>
					<description></description>
					<defaultValue>true</defaultValue>
					<help></help>
					<properties>
						<property>
							<name>readonly</name>
							<value></value>
							<type>boolean</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[]]></value>
							<type>boolean</type>
						</constraint>
					</constraints>
				</field>
			</fields>
		</section>
		<section>
			<title>Metadata</title>
			<description></description>
			<defaultOpen>false</defaultOpen>
			<fields>
				<field>
					<type>input</type>
					<id>title</id>
					<iceId></iceId>
					<title>Title</title>
					<description></description>
					<defaultValue></defaultValue>
					<help>Field Help</help>
					<properties>
						<property>
							<name>size</name>
							<value>100</value>
							<type>int</type>
						</property>
						<property>
							<name>maxlength</name>
							<value>100</value>
							<type>int</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[true]]></value>
							<type>int</type>
						</constraint>
					</constraints>
				</field>
				<field>
					<type>textarea</type>
					<id>meta-description</id>
					<iceId></iceId>
					<title>Description</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
					<properties>
						<property>
							<name>size</name>
							<value>100</value>
							<type>int</type>
						</property>
						<property>
							<name>maxlength</name>
							<value>100</value>
							<type>int</type>
						</property>
						<property>
							<name>allowResize</name>
							<value>true</value>
							<type>boolean</type>
						</property>
						<property>
							<name>cols</name>
							<value>60</value>
							<type>int</type>
						</property>
						<property>
							<name>rows</name>
							<value>10</value>
							<type>int</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[]]></value>
							<type>int</type>
						</constraint>
					</constraints>
				</field>
				<field>
					<type>input</type>
					<id>meta-keywords</id>
					<iceId></iceId>
					<title>Keywords</title>
					<description></description>
					<defaultValue></defaultValue>
					<help></help>
					<properties>
						<property>
							<name>size</name>
							<value>100</value>
							<type>int</type>
						</property>
						<property>
							<name>maxlength</name>
							<value>100</value>
							<type>int</type>
						</property>
					</properties>
					<constraints>
						<constraint>
							<name>required</name>
							<value><![CDATA[true]]></value>
							<type>int</type>
						</constraint>
					</constraints>
				</field>
			</fields>
		</section>
	</sections>
	<datasources>				<datasource>
					<type>img-desktop-upload</type>
					<id>homepage-heros</id>
					<title>Home Page Hero Images</title>
					<interface>image</interface>
					<properties>
						<property>
							<name>repoPath</name>
							<value>/static-assets/images/home-page-heros</value>
								<type>undefined</type>
						</property>
					</properties>
				</datasource>
	</datasources>
</form>
